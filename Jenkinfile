pipeline {
    agent any
    stages {

        stage('Compile') {
            steps {
                echo "Building the repository"
				 bat 'mvn clean compile'
            }
        }
        stage('Test') {
            steps {
                echo "Testing the tests"
				bat 'mvn test'
            }
			}
		stage('Deploy') {
            steps {
                echo "Deploying the tests"
				 bat 'mvn deploy'
            }
        }
    }
}